<div class="result clearfix" >
  
  <%debugger; @user = Facebooker::User.new(status_item['id']) %>
  <table >
    <td><%=fb_profile_pic(@user, :size => :square)%></td>
    <td>    
      <dl>
        <dt>
          <dd class="result_name fn"> User: <%=link_to @user.name , @user.profile_url %></dd>
          <dd class="result_name fn">Turn Name: <%= status_item['turnName'] %></dd>
          <dd class="result_name fn">Turn Id: <%= status_item['turnId'] %></dd>
          <dd class="result_name fn">Turn Number: <%= status_item['turnNumber'] %></dd>
          <dd class="result_name fn">Hint Number: <%= status_item['hintNumber'] %></dd>
        </dt>
     </dl>
     </td>
  </table>
</div>
