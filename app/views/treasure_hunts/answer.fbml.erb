<script type="text/javascript" language="JavaScript">
 function show(div){
   
  var a = document.getElementById('video_info');
  a.setStyle('display','none');
  var b = document.getElementById('geoloc_info');
  b.setStyle('display','none');
  var c = document.getElementById('picture_info');
  c.setStyle('display','none');
  var d = document.getElementById('url_info');
  d.setStyle('display','none');
  var e = document.getElementById('string_info');
  e.setStyle('display','none');
  
  
  var curr_info = document.getElementById(div);
  curr_info.setStyle('display','block');
  curr_info.refresh();
  a.refresh();
  b.refresh();
  c.refresh();
  d.refresh(); 
  e.refresh();
 }
</script>

